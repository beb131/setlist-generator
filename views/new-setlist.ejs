<% include partials/header %>

<div class="container">
    <h1>Generate New Setlist For <%= band %></h1>
    <div class="row">

        <form action="/setlists" method="POST">
            <div class="form-group">
                <input class="form-control" type="text" placeholder="venue" name="venue">
            </div>
            <div class="form-group">
                <input class="form-control" type="date" placeholder="date" name="date">
            </div>
            <div class="form-group">                
                <input class="form-control" type="number" placeholder="number of sets" name="sets">
            </div>
            <div class="form-group">
                <input class="form-control" type="number" placeholder="length per set" name="setLength">
            </div>
            <div class="form-group">
                <button class="btn btn-lg btn-primary">Submit</button>
            </div>
        </form>
    </div>
</div>

<% include partials/footer %>