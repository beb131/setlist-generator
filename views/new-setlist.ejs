<% include partials/header %>

<h1>Generate New Setlist For <%= band %></h1>

<form action="/setlists" method="POST">
    <input type="text" placeholder="venue" name="venue">
    <input type="date" placeholder="date" name="date">
    <input type="number" placeholder="number of sets" name="sets">
    <input type="number" placeholder="length per set" name="setLength">
    <button>Submit</button>
</form>

<% include partials/footer %>